{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "lib": ["dom",
      "es2015",
      "es2018.promise"],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/dto/*": [
        "./src/models/dto/*"
      ],
    }
  }
}